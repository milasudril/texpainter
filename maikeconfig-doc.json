{
	"directoryoptions": {
		"paths_full_reject": [],
		"paths_reject": [
			"..",
			".",
			".git",
			"__targets_gcov",
			"__targets_dbg",
			"__targets_rel",
			"__targets_doc"
		],
		"recursive": 1
	},
	"source_files": [
		"."
	],
	"target_hooks": [
		{
			"config": {
				"command": {
					"args": [
						"--mathml",
						"--columns",
						"10000",
						"--include-in-header",
						"user_guide/page_header",
						"--metadata",
						"pagetitle=Texpainter user guide",
						"-o",
						"{target}",
						"{source}"
					],
					"name": "pandoc"
				},
				"dependencies": [
					{
						"ref": "maikeconfig-doc.json",
						"rel": "misc"
					},
					{
						"ref": "user_guide/page_header",
						"rel": "misc"
					}
				],
				"filename_ext": ".html"
			},
			"filename_exts": [
				".md"
			],
			"name": "markdown",
			"plugin": "targettrivial"
		},
		{
			"config": {
				"command": {
					"args": [
						"{source}",
						"{target}"
					],
					"name": "cp"
				},
				"dependencies": [
					{
						"ref": "maikeconfig-doc.json",
						"rel": "misc"
					}
				],
				"filename_ext": ".png"
			},
			"filename_exts": [
				".png"
			],
			"name": "imgconv",
			"plugin": "targettrivial"
		}
	],
	"targetinfo": {
		"project_root": "",
		"target_directory": "__targets_doc"
	}
}
