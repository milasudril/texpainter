{
	"directoryoptions": {
		"paths_full_reject": [],
		"paths_reject": [
			"..",
			".",
			".git",
			"__targets_gcov",
			"__targets_dbg",
			"__targets_rel",
			"__targets_doc",
			"samples",
			"pcg-cpp",
			"libenum"
		],
		"recursive": 1
	},
	"source_files": [
		"."
	],
	"target_hooks": [
		{
			"config": {
				"command": {
					"args": [
						"{source}",
						"{target_directory}",
						"--mathml",
						"--columns",
						"10000",
						"-o",
						"{target}"
					],
					"name": "./devtools/make_doc_page.py"
				},
				"dependencies": [
					{
						"ref": "buildconfig/doc.json",
						"rel": "misc"
					},
					{
						"ref": "./devtools/vcs_info.py",
						"rel": "misc"
					}
				],
				"filename_ext": ".html"
			},
			"filename_exts": [
				".md",
				".projinfo.json"
			],
			"name": "docpage",
			"plugin": "targettrivial"
		},
		{
			"config": {
				"command": {
					"args": [
						"{source}",
						"{target}"
					],
					"name": "cp"
				},
				"dependencies": [
					{
						"ref": "buildconfig/doc.json",
						"rel": "misc"
					}
				],
				"filename_ext": ".png"
			},
			"filename_exts": [
				".png"
			],
			"name": "imgconv",
			"plugin": "targettrivial"
		},
		{
			"config": {
				"command": {
					"args": [
						"{source}",
						"{target}"
					],
					"name": "cp"
				},
				"dependencies": [
					{
						"ref": "buildconfig/doc.json",
						"rel": "misc"
					}
				],
				"filename_ext": ".css"
			},
			"filename_exts": [
				".css"
			],
			"name": "csscopy",
			"plugin": "targettrivial"
		},
		{
			"config": {
				"command": {
					"args": [
						"{source}",
						"{target}",
						"{target_directory}"
					],
					"name": "devtools/txp_render.sh"
				},
				"dependencies": [
					{
						"ref": "buildconfig/doc.json",
						"rel": "misc"
					}
				],
				"filename_ext": ".png"
			},
			"filename_exts": [
				".txp"
			],
			"name": "txprender",
			"plugin": "targettrivial"
		}
	],
	"targetinfo": {
		"project_root": "",
		"target_directory": "__targets_doc"
	}
}
